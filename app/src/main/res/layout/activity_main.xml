<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">
    <Button
        android:id="@+id/btnDisposable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Disposable"
        />
    <Button
        android:id="@+id/btnOperator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Operator(Filtered by b)"
        />
    <Button
        android:id="@+id/btnCompositeDisposible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Composite Disposible"
        />
    <Button
        android:id="@+id/btnCustomizeType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Customize Data Types"
        />

</LinearLayout>